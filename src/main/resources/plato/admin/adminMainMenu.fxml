<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="625.0" prefWidth="1020.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="view.admin.AdminMainMenu">
    <children>
      <TableView fx:id="suggestionList" editable="true" layoutX="15.0" layoutY="15.0" prefHeight="600.0" prefWidth="560.0">
        <columns>
          <TableColumn maxWidth="-1.0" minWidth="-1.0" prefWidth="-1.0" text="C2" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <contextMenu>
            <ContextMenu>
              <items>
                <MenuItem mnemonicParsing="false" onAction="#add" text="Add" />
                  <MenuItem mnemonicParsing="false" onAction="#delete" text="Delete" />
              </items>
            </ContextMenu>
         </contextMenu>
      </TableView>
    </children>
</Pane>
