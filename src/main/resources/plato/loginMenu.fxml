<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginMenu">
   <children>
      <TextField fx:id="username" layoutX="207.0" layoutY="144.0" promptText="username" />
      <PasswordField fx:id="password" layoutX="207.0" layoutY="200.0" promptText="password" />
      <Button layoutX="454.0" layoutY="284.0" mnemonicParsing="false" onAction="#loginAccount" prefHeight="31.0" prefWidth="100.0" text="Login" />
      <Button layoutX="108.0" layoutY="284.0" mnemonicParsing="false" onAction="#back" prefHeight="31.0" prefWidth="100.0" text="Back" />
      <Button layoutX="300.0" layoutY="284.0" mnemonicParsing="false" onAction="#deleteAccount" prefHeight="31.0" prefWidth="100.0" text="Delete" />
      <Label fx:id="errorMsg" alignment="CENTER" layoutX="283.0" layoutY="89.0" textAlignment="CENTER" textFill="RED" />
   </children>
</AnchorPane>
