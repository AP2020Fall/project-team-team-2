<BorderPane maxHeight="-Infinity" maxWidth="1080.0" minHeight="-Infinity" minWidth="830.0" prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.PlayerMainMenu">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
            <Menu mnemonicParsing="false" onAction="#viewMainMenu" text="Main" />
          <Menu mnemonicParsing="false" onAction="#viewAccountMenu" text="Profile" />
          <Menu mnemonicParsing="false" onAction="#viewFriendsMenu" text="Friends" />
          <Menu mnemonicParsing="false" onAction="#viewGamesMenu" text="Games" />
