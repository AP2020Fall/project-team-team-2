            <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TreeTableView>
   </center>
   <right>
      <TreeTableView fx:id="eventList" minWidth="200.0" prefHeight="200.0" BorderPane.alignment="CENTER">
